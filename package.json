
{
  "name": "webapp",
  "version": "1.0.0",
  "private": true,
  "description": "Figlinq Single Page Application",
  "main": "src/app.js",
  "scripts": {
    "clear-assets": "rm -rf assets/*",
    "build": "yarn clear-assets; webpack --mode=production --color",
    "build-dev": "yarn clear-assets; webpack --mode=development --color",
    "watch": "yarn clear-assets; webpack --mode=development --color --progress -w",
    "test": "jest test",
    "test-single-thread": "jest test --maxWorkers=1",
    "test-ci": "jest test --maxWorkers=2",
    "test-update-snapshot": "jest test --updateSnapshot",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "jest-debug": "node --inspect-brk node_modules/.bin/jest --runInBand --colors --verbose",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "prettier": "prettier --write \"src/**/*.{js,scss}\"",
    "prettier-test": "prettier -l \"src/**/*.{js,scss}\" && echo -e \"\\033[0;32m'PASS'\\033[0m\"",
    "analyze-bundle": "webpack-bundle-analyzer assets/stats.json"
  },
